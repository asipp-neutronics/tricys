{
    "paths": {
        "package_path": "example_model/package.mo",
        "db_path": "data/parameters.db",
        "results_dir": "results",
        "temp_dir": "temp"
    },
    "logging": {
        "is_dev_mode": true,
        "log_level": "INFO",
        "log_to_console": true,
        "log_count": 5,
        "log_dir": "log"
    },
    "simulation": {
        "model_name": "example_model.Cycle",
        "variableFilter": "time|sds.I\\[1]",
        "stop_time": 10000.0,
        "step_size": 1.0,
        "max_workers": 4,
        "keep_temp_files": true,
        "concurrent": false
    },
    "simulation_parameters": {
    },
    "sensitivity_analysis": {
        "enabled": true,
        "analysis_cases": [
            {
                "name": "FILE_Analysis",
                "independent_variable": "file",
                "independent_variable_sampling": "sample/sampling.csv",
                "dependent_variables": [
                    "Startup_Inventory",
                    "Self_Sufficiency_Time",
                    "Doubling_Time",
                    "Required_TBR"
                ]

            },
            {
                "name": "DIR_Analysis",
                "independent_variable": "tep_fep.to_SDS_Fraction[1]",
                "independent_variable_sampling": "linspace:0.1:0.9:2",
                "dependent_variables": [
                    "Startup_Inventory",
                    "Self_Sufficiency_Time",
                    "Doubling_Time",
                    "Required_TBR"
                ],
                "plot_type":"bar",
                "combine_plots":true,
                "sweep_time":true
            }
            ]
        ,
        "metrics_definition": {
            "Startup_Inventory": {
                "source_column": "sds.I[1]",
                "method": "calculate_startup_inventory"
            },
            "Self_Sufficiency_Time": {
                "source_column": "sds.I[1]",
                "method": "time_of_turning_point"
            },
            "Doubling_Time": {
                "source_column": "sds.I[1]",
                "method": "calculate_doubling_time"
            },
            "Required_TBR": {
                "method": "bisection_search",
                "parameter_to_optimize": "blanket.TBR",
                "search_range": [0.5,2.0],
                "tolerance": 0.005,
                "max_iterations": 10
            }
        }
    }
}