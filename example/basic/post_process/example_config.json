{
    "paths": {
        "package_path": "../../example_model/package.mo",
        "db_path": "data/parameters.db",
        "results_dir": "results",
        "temp_dir": "temp"
    },
    "logging": {
        "is_dev_mode": true,
        "log_level": "INFO",
        "log_to_console": true,
        "log_count": 5,
        "log_dir": "log"
    },
    "simulation": {
        "model_name": "example_model.Cycle",
        "variableFilter": "time|sds.I\\[1]",
        "stop_time": 100.0,
        "step_size": 0.05,
        "max_workers": 4,
        "keep_temp_files": true,
        "concurrent": false
    },
    "simulation_parameters": {
        "blanket.TBR": [1.05,1.08,1.10,1.12,1.15,1.18,1.20,1.22,1.25]
    },
    "post_processing": [
        {
            "module": "tricys.postprocess.rise_analysis",
            "function": "analyze_rise_dip",
            "params": {
                "output_filename": "rise_report.json"
            }
        },
        {
            "module": "tricys.postprocess.static_alarm",
            "function": "check_thresholds",
            "params": {
                "output_filename": "alarm_report.json",
                "rules": [
                    {
                        "columns": ["sds.I[1]"],
                        "min": 0.0
                    }
                ]
            }
        }
    ]
}
